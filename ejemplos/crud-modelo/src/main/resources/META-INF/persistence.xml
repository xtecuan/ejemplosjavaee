<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="crud-modelo_PU" transaction-type="JTA">
      
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>jdbc/alumnosDS</jta-data-source>
        <class>org.xtecuan.ejemplos.spring.crud.modelo.entidades.Usuario</class>
        <class>org.xtecuan.ejemplos.spring.crud.modelo.entidades.Libros</class>
        <class>org.xtecuan.ejemplos.spring.crud.modelo.entidades.Alumnos</class>
        <properties>
            <property name="eclipselink.target-database" value="MySQL"/>
            <property name="eclipselink.target-server" value="SunAS9"/>
            <property name="eclipselink.jpa.uppercase-column-names" value="false"/>
     
     
        </properties>
    </persistence-unit>
</persistence>
