<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
">

   
    
    
    <!--Servicio Jdbc-->
    
    <bean id="JdbcService" class="org.xtecuan.ejemplos.modelo.servicios.impl.JdbcServiceImpl">
        
        <property name="user" value="alumnos"/>
        <property name="pass" value="welcome1"/>
        <property name="driver" value="com.mysql.jdbc.Driver"/>
        <property name="url" value="jdbc:mysql://localhost:3306/alumnos?zeroDateTimeBehavior=convertToNull"/>
    </bean>   
</beans>
